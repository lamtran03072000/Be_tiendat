generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model AboutEn {
  ID    Int    @id
  title String @db.VarChar(100)
}

model AboutVn {
  ID    Int    @id
  title String @db.VarChar(100)
}

model homepage {
  id     Int  @id @default(autoincrement())
  banner Json
}
